{
  "version": "v3.1.0",
  "changelog": [
    "新增在线热更新功能"
  ],
  "mappings": [
    {
      "source": "server",
      "target": "/app",
      "exclude": [
        "*.pyc",
        "__pycache__",
        "*.backup",
        ".env"
      ]
    },
    {
      "source": "webui/dist",
      "target": "/app/dist",
      "exclude": []
    },
    {
      "source": "batch-enhancer/batch-enhancer",
      "target": "/app/batch-enhancer",
      "executable": true
    }
  ],
  "preserve": [
    "/app/data/config.json",
    "/app/data/pt_stats.db",
    "/app/data/tmp"
  ]
}
