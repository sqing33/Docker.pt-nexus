{
  "version": "v3.1.1",
  "changelog": [
    "修复：一种多站点击转种按钮没有反应的问题"
  ],
  "mappings": [
    {
      "source": "server",
      "target": "/app",
      "exclude": [
        "*.pyc",
        "__pycache__",
        "*.backup",
        ".env"
      ]
    },
    {
      "source": "webui/dist",
      "target": "/app/dist",
      "exclude": []
    },
    {
      "source": "batch/batch",
      "target": "/app/batch",
      "executable": true
    }
  ],
  "preserve": [
    "/app/data/config.json",
    "/app/data/pt_stats.db",
    "/app/data/tmp"
  ]
}
